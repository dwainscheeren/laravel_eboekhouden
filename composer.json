{
    "name": "daanvanberkel/laravel_eboekhouden",
    "description": "Eboekhouden provider for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Daan van Berkel",
            "email": "info@daanvanberkel.nl"
        }
    ],
    "require": {
        "daanvanberkel/laravel_accounting": "dev-master",
        "ext-soap": "*",
        "php": "^7.3|^8.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/dwainscheeren/laravel_accounting"
        }
    ],
    "autoload": {
        "psr-4": {
            "Dvb\\Eboekhouden\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dvb\\Eboekhouden\\ServiceProvider"
            ]
        }
    }
}
